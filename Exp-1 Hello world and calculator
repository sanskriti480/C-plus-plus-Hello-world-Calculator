//Program 2

#include <iostream>
using namespace std;

int main(){
    float num1, num2, sum, div, sub, multiply;
    cout <<"Enter first number: ";
    cin >> num1;
    cout <<"Enter second number: ";
    cin >> num2;
    
    sum = num1 + num2;
    cout << "sum = " << sum <<endl;
    
    sub = num1 - num2;
    cout << "sub = " << sub <<endl;
    
    multiply = num1 * num2;
    cout << "multiply = " << multiply <<endl;
    
    div = num1 / num2;
    cout << "div = " << div <<endl;

return 0;
}

//Output
//Program 1

//Hello World

//Program 2

//Enter first number: 80
//Enter second number: 40
//sum = 120
//sub = 40
//multiply = 3200
//div = 2

